{
  "name": "tuxedoman",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "prestart": "eris-boiler migrations latest",
    "postinstall": "npm dedupe",
    "lint": "eslint .",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "debug": "node --inspect=0.0.0.0:9229 -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "validate": "eslint . --fix && tsc --noEmit",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "devDependencies": {
    "@alex-taxiera/eslint-config-cjs": "^1.1.1",
    "@alex-taxiera/eslint-config-ts": "^2.0.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "ts-node-dev": "2.0.0",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "@types/config": "0.0.41",
    "@types/node": "16.x",
    "config": "^3.3.7",
    "docker-secret-env": "^0.2.1",
    "eris-boiler": "3.1.0-beta.15",
    "js-yaml": "^4.1.0",
    "knex": "0.21.21",
    "pg": "8.7.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
