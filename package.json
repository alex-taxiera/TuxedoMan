{
  "name": "tuxedoman",
  "version": "1.0.0",
  "description": "Discord presence based role manager",
  "main": "./index.js",
  "scripts": {
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "build": "npx tsc",
    "dev": "node --inspect-brk=5858 -r ./bin/ts-node.js ./src/index.ts",
    "dev:watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-taxiera/TuxedoMan.git"
  },
  "keywords": [
    "discord",
    "bot",
    "roles",
    "community",
    "gaming"
  ],
  "author": "Alex Taxiera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alex-taxiera/TuxedoMan/issues"
  },
  "homepage": "https://github.com/alex-taxiera/TuxedoMan#readme",
  "engines": {
    "node": "10.5.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.4",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "eris-boiler": "github:alex-taxiera/eris-boiler#release/3.0.0"
  }
}
