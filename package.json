{
  "name": "tuxedoman",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "prestart": "knex migrate:latest",
    "postinstall": "npm dedupe",
    "lint": "eslint .",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "debug": "node --inspect=0.0.0.0:9229 -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "validate": "npm run lint && tsc --noEmit",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@alex-taxiera/eslint-config-ts": "file:../eslint-config/packages/ts",
    "@types/ws": "^8.5.13",
    "eslint": "^9.16.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "ts-node-dev": "2.0.0"
  },
  "dependencies": {
    "@hephaestus/eris": "^1.0.0-alpha.15",
    "@sentry/node": "^8.43.0",
    "@sentry/tracing": "^7.114.0",
    "@types/config": "3.3.5",
    "@types/node": "20.x",
    "config": "^3.3.12",
    "docker-secret-env": "^0.2.1",
    "eris-boiler": "3.1.0-beta.16",
    "js-yaml": "^4.1.0",
    "knex": "0.21.21",
    "pg": "8.13.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.7.2"
  }
}
